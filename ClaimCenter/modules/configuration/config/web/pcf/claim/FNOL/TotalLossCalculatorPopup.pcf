<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Popup
    beforeCommit="VehicleIncident.TotalLossCalculatorPreUpdate()"
    canEdit="true"
    id="TotalLossCalculatorPopup"
    startInEditMode="true"
    title="displaykey.Web.TotalLossCalculatorPopup.Title">
    <LocationEntryPoint
      signature="TotalLossCalculatorPopup(VehicleIncident : VehicleIncident)"/>
    <Variable
      name="VehicleIncident"
      type="VehicleIncident"/>
    <Screen>
      <Toolbar>
        <EditButtons/>
      </Toolbar>
      <DetailViewPanel>
        <InputColumn>
          <Label
            id="VehicleLabel"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.Vehicle"/>
          <Input
            id="VehicleYear"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleYear"
            value="VehicleIncident.Vehicle.Year"/>
          <Input
            id="VehicleMake"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleMake"
            value="VehicleIncident.Vehicle.Make"/>
          <Input
            id="VehicleModel"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleModel"
            value="VehicleIncident.Vehicle.Model"/>
          <TypeKeyInput
            id="VehicleStyle"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleStyle"
            value="VehicleIncident.Vehicle.Style"/>
          <Input
            id="VehicleColor"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleColor"
            value="VehicleIncident.Vehicle.Color"/>
          <Input
            id="VehicleVin"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleVin"
            value="VehicleIncident.Vehicle.Vin"/>
          <Input
            id="VehicleLicensePlate"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleLicensePlate"
            value="VehicleIncident.Vehicle.LicensePlate"/>
          <TypeKeyInput
            filter="VALUE.hasCategory(JurisdictionType.TC_VEHICLE_REG)"
            id="VehicleLicensePlateState"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleLicensePlateState"
            value="VehicleIncident.Vehicle.State"/>
          <InputDivider/>
          <Input
            boldLabel="true"
            id="TotalLossPoints"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.TotalLossPoints"
            required="false"
            value="VehicleIncident.TotalLossPoints"/>
          <BooleanRadioInput
            boldLabel="true"
            id="VehicleTotalLoss"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleTotalLoss"
            required="false"
            value="VehicleIncident.TotalLoss"/>
          <InputDivider/>
          <Label
            id="VehInfoLabel"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.HowVehDamaged"/>
          <BooleanRadioInput
            editable="true"
            id="VehicleAge5Years"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleAge5Years"
            required="false"
            value="VehicleIncident.VehicleAge5Years"/>
          <BooleanRadioInput
            editable="true"
            id="VehicleAge10Years"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleAge10Years"
            required="false"
            value="VehicleIncident.VehicleAge10Years"/>
          <BooleanRadioInput
            editable="true"
            id="Mileage100K"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.Mileage100K"
            required="false"
            value="VehicleIncident.Mileage100K"/>
          <BooleanRadioInput
            id="VehicleTowed"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleTowed"
            required="false"
            value="VehicleIncident.HasTowingServices"/>
          <BooleanRadioInput
            editable="true"
            id="Extrication"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.Extrication"
            required="false"
            value="VehicleIncident.Extrication"/>
          <InputDivider/>
          <Label
            id="CollisionLabel"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.CollisionSection"/>
          <BooleanRadioInput
            id="VehicleAirbagsDeployed"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleAirbagsDeployed"
            required="false"
            value="VehicleIncident.AirbagsDeployed"/>
          <BooleanRadioInput
            editable="true"
            id="VehicleRollOver"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleRollOver"
            required="false"
            value="VehicleIncident.VehicleRollOver"/>
          <InputDivider/>
          <Label
            id="FireLabel"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.FireSection"/>
          <BooleanRadioInput
            editable="true"
            id="FireBurnDash"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.FireBurnDash"
            required="false"
            value="VehicleIncident.FireBurnDash"/>
          <BooleanRadioInput
            editable="true"
            id="FireBurnEngine"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.FireBurnEngine"
            required="false"
            value="VehicleIncident.FireBurnEngine"/>
          <BooleanRadioInput
            editable="true"
            id="FireBurnWindshield"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.FireBurnWindshield"
            required="false"
            value="VehicleIncident.FireBurnWindshield"/>
          <InputDivider/>
          <Label
            id="FloodLabel"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.FloodSection"/>
          <BooleanRadioInput
            editable="true"
            id="VehicleSubmerged"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VehicleSubmerged"
            required="false"
            value="VehicleIncident.VehicleSubmerged"/>
          <BooleanRadioInput
            editable="true"
            id="WaterLevelDash"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.WaterLevelDash"
            required="false"
            value="VehicleIncident.WaterLevelDash"/>
          <BooleanRadioInput
            editable="true"
            id="FloodSaltWater"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.FloodSaltWater"
            required="false"
            value="VehicleIncident.FloodSaltWater"/>
          <BooleanRadioInput
            editable="true"
            id="WaterLevelSeats"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.WaterLevelSeats"
            required="false"
            value="VehicleIncident.WaterLevelSeats"/>
          <InputDivider/>
          <Label
            id="TheftLabel"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.VandalTheftSection"/>
          <BooleanRadioInput
            editable="true"
            id="ComponentsMissing"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.ComponentsMissing"
            required="false"
            value="VehicleIncident.ComponentsMissing"/>
          <BooleanRadioInput
            editable="true"
            id="InteriorMissing"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.InteriorMissing"
            required="false"
            value="VehicleIncident.InteriorMissing"/>
          <BooleanRadioInput
            editable="true"
            id="AirbagsMissing"
            label="displaykey.NVV.Exposure.SubView.TotalLossCalculator.AirbagsMissing"
            required="false"
            value="VehicleIncident.AirbagsMissing"/>
        </InputColumn>
      </DetailViewPanel>
    </Screen>
  </Popup>
</PCF>