<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <Screen
    id="PolicyRefresh_PickPropertyRiskUnitsScreen">
    <Require
      name="wizard"
      type="gw.api.policy.refresh.PolicyRefreshAlgorithm"/>
    <Require
      name="claim"
      type="Claim"/>
    <Require
      name="policySummary"
      type="PolicySummary"/>
    <Toolbar
      id="Toolbar">
      <WizardButtons
        id="Buttons"/>
    </Toolbar>
    <PanelRef
      def="PolicyRefreshExistingPropertyRULV(claim,policySummary)">
      <TitleBar
        title="displaykey.Web.PolicyRefresh.CurrentProperties"/>
      <Toolbar/>
    </PanelRef>
    <PanelSet>
      <Verbatim
        label="&quot;   &quot;"/>
    </PanelSet>
    <PanelRef
      def="PolicySummaryPropertyLV( policySummary, true, true )"
      editable="true">
      <TitleBar
        title="displaykey.Web.PolicyRefresh.PropertiesFromPolicySystem"/>
      <Toolbar/>
      <Verbatim/>
    </PanelRef>
  </Screen>
</PCF>