<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">
	<bean id="studentBean" class="d2com.pgm.dependencycheck.Student"
		autowire="byType" dependency-check="objects">
		<property name="id" value="1234"></property>
		<property name="name" value="vikas"></property>
	</bean>
	<bean id="addressBean" class="d2com.pgm.dependencycheck.Address">
		<property name="addressLine1" value="kabilar street"></property>
		<property name="addressLine2" value="palvanthangal"></property>
		<property name="zipCode" value="229306"></property>

	</bean>


</beans>