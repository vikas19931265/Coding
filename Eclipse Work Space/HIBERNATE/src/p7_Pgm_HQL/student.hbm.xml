<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="p7_Pgm_HQL.Student" table="TB_STUDENT">
		<id name="sno" column="NO" type="integer" length="4">
		<generator class="increment"></generator>
		</id>
		<version name="version" column="VERSION"></version>
		<property name="sname" column="NAME" type="string" length="20"></property>
		<property name="semail" column="EMAIL" type="string" length="25"></property>
		<property name="smobile" column="MOBILE" type="string"
			length="15"></property>
	</class>


</hibernate-mapping>
