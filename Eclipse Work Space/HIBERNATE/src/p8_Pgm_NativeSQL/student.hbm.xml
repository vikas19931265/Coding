<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="p8_Pgm_NativeSQL.Student" table="TB_STUDENT">
		<id name="sno" column="NO" type="integer" length="4">
		<generator class="increment"></generator>
		</id>
		<version name="version" column="VERSION"></version>
		<property name="sname" column="NAME" type="string" length="20"></property>
		<property name="semail" column="EMAIL" type="string" length="25"></property>
		<property name="smobile" column="MOBILE" type="string"
			length="15"></property>
	</class>
	<sql-query name="GET_DETAILS">
	<return-scalar column="no" type="integer"/>
	<return-scalar column="email" type="string"/>
	<![CDATA[ select no,email from TB_STUDENT where no < :sno1]]>
	
	
<!-- 
CDATA is very useful to tell the xml parser to ignore parsing of this data..otherwise it will treat < as
starting angular bracket.

 -->
	</sql-query>
	
	<sql-query name="select_all_column_sql">
		<return class="p8_Pgm_NativeSQL.Student"></return>
	select * from TB_STUDENT
	</sql-query>


</hibernate-mapping>
