<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="q5_Pgm_Association_1ToM_TeamPlayer.Team" table="TB_TEAM">
		<id name="teamId">
			<generator class="increment"></generator>
		</id>
		
<property name="teamName" type="string" length="20"></property>

<set name="players" cascade="all">
<key column="teamId"></key>
<one-to-many class="q5_Pgm_Association_1ToM_TeamPlayer.Players"/>

</set>

	</class>


</hibernate-mapping>