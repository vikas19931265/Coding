<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Units for claim metrics, such as days, currency, percent etc."
  name="MetricUnit">
  <typecode
    code="numeric"
    desc="Metric is a simple number"
    name="Numeric"
    priority="1"/>
  <typecode
    code="percent"
    desc="Metric is measured in percent"
    name="Percent"
    priority="2"/>
  <typecode
    code="currency"
    desc="Metric is measured in a particular currency"
    name="Currency"
    priority="3"/>
  <typecode
    code="hours"
    desc="Metric is measured in hours"
    name="Hours"
    priority="4"/>
  <typecode
          code="days"
          desc="Metric is measured in days"
          name="Days"
          priority="5"/>
</typelistextension>